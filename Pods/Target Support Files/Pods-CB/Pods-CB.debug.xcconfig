ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/AVOSCloud" "$PODS_CONFIGURATION_BUILD_DIR/Hero" "$PODS_CONFIGURATION_BUILD_DIR/Koloda" "$PODS_CONFIGURATION_BUILD_DIR/Pastel" "$PODS_CONFIGURATION_BUILD_DIR/RevealingSplashView" "$PODS_CONFIGURATION_BUILD_DIR/pop"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/AVOSCloud/AVOSCloud.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Hero/Hero.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Koloda/Koloda.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Pastel/Pastel.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/RevealingSplashView/RevealingSplashView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/pop/pop.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AVOSCloud" -framework "Hero" -framework "Koloda" -framework "Pastel" -framework "RevealingSplashView" -framework "pop"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
